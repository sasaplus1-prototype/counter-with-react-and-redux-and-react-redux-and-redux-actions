{
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "fixpack": "^2.3.1",
    "prop-types": "^15.6.0",
    "webpack": "^3.6.0"
  },
  "private": true,
  "scripts": {
    "build": "webpack --colors --display-error-details --progress",
    "build:watch": "npm run build -- --devtool inline-source-map --watch",
    "fixpack": "fixpack"
  }
}
